  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "concurrently": "^7.0.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.8"
  },
